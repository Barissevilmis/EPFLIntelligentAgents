<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
		<set discount-factor=".85"/>
	</agent>
	
	<agent name="reactive-dummy">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveDummy"/>
	</agent>
	
	<agent name="reactive-rla">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set discount-factor=".85"/>
	</agent>
	
	<agent name="reactive-rla-d0">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set discount-factor="0"/>
	</agent>
	
	<agent name="reactive-rla-d0.1">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set discount-factor="0.1"/>
	</agent>
	
	<agent name="reactive-rla-d0.5">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set discount-factor="0.5"/>
	</agent>
	
	<agent name="reactive-rla-d0.99">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set discount-factor="0.99"/>
	</agent>
	
	<agent name="reactive-rla-eps-1e2">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set epsilon="100"/>
	</agent>
	
	<agent name="reactive-rla-eps-1e-2">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set epsilon=".01"/>
	</agent>
	
	<agent name="reactive-rla-eps-1e-6">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set epsilon=".000001"/>
	</agent>
	
	<agent name="reactive-rla-e1e4">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set epsilon="10000"/>
	</agent>
	
	<agent name="reactive-rla-e1e0">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set epsilon="1"/>
	</agent>
	
	<agent name="reactive-rla-e1e-4">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRLA"/>
		<set epsilon="0.0001"/>
	</agent>
	
</agents>
